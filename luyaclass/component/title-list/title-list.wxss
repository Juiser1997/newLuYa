.article{margin:0 24rpx;box-sizing: border-box;}
.article-item{display: flex;padding:  24rpx 0;border-bottom: 1rpx solid #ededed;justify-content: space-between;}
.article-item-left{width: 430rpx;}
.article-item-title{font-size: 32rpx;line-height: 42rpx;display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 2;overflow: hidden;height: 84rpx;}
.article-item-bottom{font-size: 22rpx;color:#999;margin-top:25rpx;display: flex;}
.article-item-bottom text{margin-right: 15rpx;}
.article-item-bottom .brand{font-size: 18rpx;height: 26rpx;width: 48rpx;line-height: 26rpx;text-align: center;color:#4b6dff;border:1rpx solid #4b6dff;border-radius: 6rpx;}
.article-item-right{width: 234rpx;height: 146rpx;border-radius: 5rpx;}
.fire{font-size:24rpx;color:#f55b4e;}
