.head{position: fixed;top:0;left: 0;background-color: #fff;width: 100%;height: 100rpx;z-index: 100;}
.cont{margin-top: 100rpx;}
.banner-box{height: 350rpx;width: 700rpx;border-radius: 10rpx;margin: 0 auto;}
.swiper{height: 350rpx;width: 700rpx;}
.swiper-item{width: 100%;height: 100%;}
.banner-box image{border-radius: 10rpx;height: 350rpx;width: 700rpx;}
.mid-nav{height: 215rpx;display: flex;border-bottom:1rpx solid #ededed;color:#666;font-size: 24rpx;}
.mid-nav-item{flex: auto;display: flex;flex-direction: column;align-items: center;align-self: center;}
.mid-nav-item image{height: 122rpx;width: 122rpx;}
.Event-item>image{width: 371rpx;height: 91rpx;}
.Event{display: flex;margin: 20rpx 0;padding: 0 2rpx;justify-content: space-between;}
.Eventsai-item>image{width: 256rpx;height: 104rpx;}
.Eventsai{display: flex;margin: 20rpx 0;padding: 0 2rpx;justify-content: space-between;}
.Eventvip>image{width: 224rpx;height: 104rpx;}
.tips{width: 700rpx;height: 80rpx;line-height: 80rpx;position: fixed;top:25rpx;left:25rpx;color: #fff;
    background-color: #2CCD92;border-radius: 80rpx;display: flex;z-index: 1100;font-size: 28rpx;justify-content: center;}
.icon-san{width: 34rpx;height: 12rpx;align-self: center;margin: 0 10rpx;}
.icon-cha{width: 20rpx;height: 20rpx;align-self: center;}
.sanjiao{width: 0;height: 0;border: 15rpx solid;border-color: transparent transparent #2CCD92;position: fixed;top:0;right: 100rpx;z-index: 1100;}
.nodata{height: 80rpx;width: 100%;color:#999;display: flex;box-sizing: border-box;line-height: 80rpx;font-size: 28rpx;justify-content: center;border-radius: 8rpx;}